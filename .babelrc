{
    "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-flow"],
    "plugins": [
        "relay",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-syntax-dynamic-import",
        ["module-resolver", {
            "alias": {
                "foundation": "./foundation"
            },
        }],
    ]
}
