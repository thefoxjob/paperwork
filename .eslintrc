{
    "extends": "airbnb",
    "rules": {
        "max-len": [2, 180],
        "react/jsx-curly-spacing": [2, "always", { "spacing": { "objectLiterals": "never" }}],
        "space-in-parens": [0, "always"],
        "space-unary-ops": [2, { "words": true, "nonwords": true }],
        "template-curly-spacing": [2, "always"]
    }
}
